    <script type="text/javascript">
        function SetAanhef() {
            aanhefPart = document.FormNAME.elements['titelatuur'].options[titelatuur.selectedIndex].innerHTML.replace('De ', '');
            aanhefPart = aanhefPart.toLowerCase();
            document.FormNAME.elements['aanhef'].value = aanhefPart + ' ' + document.getElementById('achternaam')
                .value;
            var ifrm = document.getElementById('NieuwKlantNr');
            var win = ifrm.contentWindow;
            var doc = ifrm.contentDocument ? ifrm.contentDocument : ifrm.contentWindow.document;
            // input overzetten naar iframe en laten zoeken naar klantnummer
            var fld = doc.forms['myform'].elements['country'];
            var naam = document.getElementById('achternaam')
                .value.replace('van ', '');
            naam = naam.replace('de ', '');
            naam = naam.replace('der ', '');
            naam = naam.replace('den ', '');
            fld.value = naam.substring(0, 4);
            //functie suggest aansturen met eerste 4 karakters van de achternaam
            document.getElementById('NieuwKlantNr')
                .contentWindow.suggest(fld.value);

        }

    </script>
    <script type="text/javascript">
        function ControlForm() {

            //controles verwerken
            var nota = document.getElementById('notification');
            nota.style.position = "absolute";
            nota.style.left = "0px";
            nota.style.top = "10%";
            nota.style.margin = "0px 0 0 0px";
            nota.style.width = "100%";
            nota.style.height = "600px";
            nota.style.padding = "30px 30% 30px 30%";
            nota.style.backgroundColor = "rgba(240,240,240,0.90)";
            nota.style.border = "1px solid #ccc";
            nota.style.borderRadius = "0px";
            //nota.style.visibility= 'visible';


            if (window.frames['kixcheck'].document.body.innerHTML.search('WAARSCHUWING!') > 0) {
                nota.style.visibility = "visible";
                var dubbelkix = window.frames['kixcheck'].document.body.innerHTML.search('WAARSCHUWING!');
                nota.innerHTML = nota.innerHTML + '<hr><b>' + (window.frames['kixcheck'].document.body.innerHTML.substring(dubbelkix)) + '</b><hr>';
            } else

            if (window.frames['emailcheck'].document.body.innerHTML.search('<hr>0<hr>') > 2) {
                nota.style.visibility = "visible";
                nota.innerHTML = nota.innerHTML + (document.getElementById('Email')
                    .value + ' is geen geldig e-mail adres omdat de samenstelling niet klopt of omdat het geen geldige mailserver naam is.');
            } else

            if (window.frames['emailcheck'].document.body.innerHTML.search('<hr>-1<hr>') > 2) {
                nota.style.visibility = "visible";
                nota.innerHTML = nota.innerHTML + (document.getElementById('Email')
                    .value + ' kan niet als geldig e-mail adres bepaadt worden. De e-mail server reageerd niet.');
            } else

            if (window.frames['emailcheck'].document.body.innerHTML.search('WAARSCHUWING!') > 0) {
                nota.style.visibility = "visible";
                var dubbelemail = window.frames['emailcheck'].document.body.innerHTML.search('WAARSCHUWING!');
                nota.innerHTML = nota.innerHTML + '<hr><b>' + (window.frames['emailcheck'].document.body.innerHTML.substring(dubbelemail)) + '</b><hr>';
            }

            InNavCheck = window.frames['emailcheck'].document.body.innerHTML.search('WAARSCHUWING!') + window.frames['kixcheck'].document.body.innerHTML.search('WAARSCHUWING!');

            //alert(InNavCheck);
            //alert(window.frames['emailcheck'].document.readyState);

            if ((InNavCheck < 0) && (window.frames['emailcheck'].document.readyState = 'complete'))

            {
                document.getElementById('verder')
                    .style.visibility = 'hidden';
                document.getElementById('InNav')
                    .style.visibility = 'visible';
            }

        }

    </script>

    <script type="text/javascript">
        function InhoudForm() {
            //formwaarden indien geen waarschuwing opnemen
            var params;
            var qline = 'create table nk as select ';
            for (i = 0; i < document.FormNAME.elements.length; i++) {
                var fieldName = document.FormNAME.elements[i].name;
                var fieldValue = document.FormNAME.elements[i].value;
                var fieldType = document.FormNAME.elements[i].type;

                // use the fields, put them in a array, etc.

                // or, add them to a key-value pair strings, 
                // as in regular POST
                if ((typeof fieldName !== "undefined")) {
                    params += fieldName + '=' + fieldValue + '&';



                    if (fieldName != '') {

                        if (fieldType == 'checkbox') {
                            fieldValue = document.FormNAME.elements[fieldName].checked;
                        }

                        qline += ' "' + fieldValue + '" as `' + fieldName + '`, ';
                    }
                }
            }

            qline += ' "' + document.getElementById('postcode')
                .value.replace(' ', '') + document.FormNAME.elements["huisnummer"].value + document.FormNAME.elements["Toevoeging"].value + '" as `KIX`;'

            if (document.getElementById("nk")) {
                elem.parentElement.removeChild(elem);
            }

            var qframe = document.createElement("iframe")
                , iframeWindow;

            qframe.src = "nk.php?q=" + qline;
            qframe.id = "nk";
            qframe.name = "nk";
            qframe.style.position = "absolute";
            qframe.style.top = "50%";
            qframe.style.left = "50%";
            qframe.style.visibility = "hidden";
            document.body.appendChild(qframe);
            var nota = document.getElementById('notification');
            nota.style.borderRadius = "0px";
            nota.innerHTML = '<img class="logoRotatie" src="http://ftp.sauterwijnen.nl/svg/Sauter_Ster_Wit_Outline.svg" style="height:300px;align:right;verticalAlign:middle;">'
            nota.style.visibility = 'visible';
            nota.style.backgroundColor = "rgba(200,200,200,0.70)";
            nota.style.left = "0px";
            nota.style.top = "0px";
            nota.style.margin = "0 0 0 0";
            nota.style.height = "100%";

            function hideIt() {
                document.getElementById("notification")
                    .style.visibility = "hidden";
                document.getElementById("FormID")
                    .reset();
                document.getElementById('verder')
                    .style.visibility = 'hidden';
                document.getElementById('InNav')
                    .style.visibility = 'hidden';

            }
            setTimeout(hideIt, 5000);
        }

    </script>

